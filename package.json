  "scripts": {
    "dev": "blocklet dev --open",
    "lint": "eslint src api",
    "lint:fix": "eslint src api --fix",
    "precommit": "npm run lint",
    "prepush": "npm run lint",
    "clean": "rm -rf dist api/dist .blocklet",
    "start": "nodemon -w api api/dev.mjs",
    "bundle": "npm run clean && npm run bundle:client && npm run bundle:server",
    "bundle:client": "vite build",
    "bundle:server": "blocklet bundle --compact --external @abtnode/auth --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "prepublishOnly": "npm run bundle",
    "release": "npm run clean && npm run bundle",
    "update:deps": "npx -y taze latest -r -w -n '/@abtnode|@aigne|@arcblock|@blocklet|@did-connect|@did-pay|@did-space|@nft-store|@nft-studio|@ocap|vite-plugin-blocklet/' -w && pnpm i",
    "dedupe": "npx yarn-deduplicate yarn.lock && yarn",
    "bump-version": "make bump-version"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
