  "scripts": {
    "lint": "eslint src api",
    "lint:fix": "eslint src api --fix",
    "precommit": "npm run lint",
    "prepush": "npm run lint",
    "clean": "rm -rf build api/dist .blocklet",
    "start": "npm-run-all --parallel start:*",
    "start:client": "PUBLIC_URL=\"\" react-app-rewired start",
    "start:server": "NODE_ENV=development DEBUG=@arcblock/* nodemon api/index.js -w api",
    "build": "npm run clean && npm-run-all build:*",
    "build:client": "react-app-rewired build && rm -f build/static/**/*.map",
    "bundle": "npm run clean && npm run bundle:client && npm run bundle:server",
    "bundle:client": "PUBLIC_URL=\"/.blocklet/proxy/z8iZjySpAu4jzbMochL9k1okuji1GcS7RRRDM\" REACT_APP_API_PREFIX=\"\" REACT_APP_APP_NAME=\"OCAP Playground\" npm run build:client",
    "bundle:server": "blocklet bundle --zip --create-release",
    "prepublishOnly": "npm run bundle",
    "release": "npm run clean && npm run bundle",
    "deploy": "npm run release && blocklet deploy .blocklet/bundle",
    "test": "react-app-rewired test",
    "echo": "export MESSAGE=$1 && echo $MESSAGE",
    "eject": "react-app-rewired eject",
    "update:deps": "ncu -u -f /arcblock\\|ocap\\|abtnode\\|blocklet/ && yarn && npx --yes yarn-deduplicate yarn.lock && yarn"
  },
